public class Main {
    public static void main(String[] args) {
        Calc calc = new Calc();

        int resultado1 = calc.somar(10, 20);
        int resultado2 = calc.somar(5, 15, 25);
        double resultado3 = calc.somar(3.5, 2.7);

        System.out.println("Soma de 10 + 20 = " + resultado1);
        System.out.println("Soma de 5 + 15 + 25 = " + resultado2);
        System.out.println("Soma de 3.5 + 2.7 = " + resultado3);
    }
}
